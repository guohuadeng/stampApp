<config>
    <modules>
        <Webshopapps_Wsacommon>
            <version>1.0.0</version>
        </Webshopapps_Wsacommon>
    </modules>
    <global>
    	<blocks>
    		<wsacommon>
    			<class>Webshopapps_Wsacommon_Block</class>
    		</wsacommon>
            <adminhtml>
                <rewrite>
                    <sales_order_create_shipping_method_form>Webshopapps_Wsacommon_Block_Adminhtml_Sales_Order_Create_Shipping_Method_Form</sales_order_create_shipping_method_form>
                </rewrite>
            </adminhtml>
    	   <wsacommon_adminhtml>
                <class>Webshopapps_Wsacommon_Block_Adminhtml</class>
            </wsacommon_adminhtml>
    	</blocks>
        <models>
            <wsacommon>
                <class>Webshopapps_Wsacommon_Model</class>
                <resourceModel>wsacommon_resource</resourceModel>
            </wsacommon>
            <wsacommon_resource>
            	<class>Webshopapps_Wsacommon_Model_Resource</class>
            </wsacommon_resource>
        </models>
		<helpers>
            <wsacommon>
            	<class>Webshopapps_Wsacommon_Helper</class>
            </wsacommon>
        </helpers>
        <resources>
            <wsacommon_setup>
                <setup>
                    <module>Webshopapps_Wsacommon</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </wsacommon_setup>
        </resources>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Webshopapps_Wsacommon before="Mage_Adminhtml">Webshopapps_Wsacommon</Webshopapps_Wsacommon>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>